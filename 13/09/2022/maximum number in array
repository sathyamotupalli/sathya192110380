#include<stdio.h>
int main()
{
	int a,b[10],i,c,d;
	printf("enter total nnumber of elements= ");
	scanf("%d",&a);
	for (i=1;i<=a;i++){
		printf("enter the values= ");
		scanf("%d",&b[i]);}
	for (i=0;i<=a;i++)
	{
		if (c<b[i]){
			c=b[i];
		}}
	for (i=0;i<=a;i++){
		if (d>b[i]){
			d=b[i];
		}}
	printf("the maximum number is =%d\n",c);
	
}
