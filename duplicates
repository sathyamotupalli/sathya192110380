#include<stdio.h>
int main()
{
    int a,b[20],i,j,c;
    printf("enter the total number of elements= ");
    scanf("%d",& a);
    for (i=0;i<a;i++){
    printf("enter the number= ");
    scanf("%d",& b[i]);}
    
    for (i=0;i<a;i++){
    for (j=i+1;j<a;++j)
    {
        if (b[i]==b[j])
        {
            for (c=i;c<a-1;c++)
            {
                b[c]=b[c+1];
            }
         a--;
         j--;
        }
    }}
    for (i=0;i<a;i++)
    {
        printf("%d\n",b[i]);
    }

}
